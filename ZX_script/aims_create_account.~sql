   ----------------
   
   
   
   
begin
  for acc in (select * from aims_account) loop
  
    insert into aims_account_balance
      (BALANCE_ID,
       ACCOUNTCODE,
       BANK_CODE,
       ACCOUNTNO,
       BALANCE_DATE,
       ACCOUNT_BALANCE,
       AVAILABLE_BALANCE,
       YESTERDAY_BALANCE,
       AVAILABLE_TUOTA,
       SOURCE_TYPE,
       CREATE_TIME)
    values
      (aims_account_balance_seq.nextval,
       acc.accountcode,
       acc.bankno,
       acc.accountno,
       sysdate,
       trunc(dbms_random.value(0, 10000)),
       trunc(dbms_random.value(0, 10000)),
       trunc(dbms_random.value(0, 10000)),
       null,
       '02',
       sysdate);
  end loop;
end;

